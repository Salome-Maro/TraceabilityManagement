<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated trace -->
      <package
            uri="org.amalthea4public.metamodel.trace"
            class="org.amalthea4public.metamodel.trace.TracePackage"
            genModel="model/trace.xcore"/>
   </extension>
   <extension
         point="org.amalthea4public.configuration.metamodel.trace">
      <trace_metamodel
            adapter="org.amalthea4public.metamodel.trace.TraceMetamodelAdapter">
